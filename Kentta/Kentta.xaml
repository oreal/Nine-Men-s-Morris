<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:nappi="clr-namespace:NappulaEllipsi;assembly=NappulaEllipsi"
             xmlns:oma="clr-namespace:Kentta"
             xmlns:NappulaEllipsi="clr-namespace:NappulaEllipsi;assembly=NappulaEllipsi" x:Class="Kentta.UserControl1" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

   

    <UserControl.Resources>
        <oma:NameConverter x:Key="GameConverter"/>
        <oma:ShowNaapurit x:Key="ShowNaapuritConverter"></oma:ShowNaapurit>
        <!--  <oma:VuoroConverter x:Key="VuoroConverter" /> -->

        <Style x:Key="FullHorizontalLine" TargetType="{x:Type Line}">
            <Setter Property="Y1" Value="{Binding HalfHeight, ElementName=canvas}"/>
            <Setter Property="X1" Value="0"/>
            <Setter Property="X2" Value="{Binding ActualWidth, ElementName=canvas}"/>
            <Setter Property="Y2" Value="{Binding HalfHeight, ElementName=canvas}"/>
            <Setter Property="Stroke" Value="Black"/>
            <Setter Property="StrokeThickness" Value="4"/>
        </Style>

        <!-- FullVerticalLine -->
        <Style x:Key="FullVerticalLine" TargetType="{x:Type Line}">
            <Setter Property="Y1" Value="{Binding Height, ElementName=canvas}"/>
            <Setter Property="X1" Value="{Binding HalfWidth, ElementName=canvas}"/>
            <Setter Property="X2" Value="{Binding HalfWidth, ElementName=canvas}"/>
            <Setter Property="Y2" Value="{Binding ActualHeight, ElementName=canvas}"/>
            <Setter Property="Stroke" Value="Black"/>
            <Setter Property="StrokeThickness" Value="4"/>
        </Style>

        <!-- Ellipsin tyylit -->
        <Style TargetType="NappulaEllipsi:UserControl1">
            <Setter Property="Height" Value="30"/>
            <Setter Property="Width" Value="30"/>
            <Setter Property="Color" Value="Black"/> 
            <Setter Property="Focusable" Value="False"/>
            <Setter Property="AllowDrop" Value="True"/>
        </Style>
        <!-- Ellipsin tyylit -->
        <Style TargetType="{x:Type Ellipse}">
            <Setter Property="Height" Value="20"/>
            <Setter Property="Width" Value="20"/>
            <Setter Property="Fill" Value="Black"/>
            <Setter Property="Focusable" Value="False"/>
            <Setter Property="AllowDrop" Value="True"/>



            <Style.Triggers>
                <MultiTrigger>
                    <MultiTrigger.Conditions>
                        <Condition Property="Focusable" Value="True" />
                        <Condition Property="IsMouseOver" Value="True" />
                        <Condition Property="Fill" Value="Black" />
                    </MultiTrigger.Conditions>
                    <Setter Property="Stroke" Value="LightGray"/>
                    <Setter Property="StrokeThickness" Value="5"/>
                </MultiTrigger>

                <MultiTrigger>
                    <MultiTrigger.Conditions>
                        <Condition Property="Focusable" Value="False" />
                        <Condition Property="IsMouseOver" Value="True" />
                        <Condition Property="Fill" Value="Tomato" />
                    </MultiTrigger.Conditions>
                    <Setter Property="Stroke" Value="LightGray"/>
                    <Setter Property="StrokeThickness" Value="5"/>
                </MultiTrigger>
            </Style.Triggers>
        </Style>

    </UserControl.Resources>


    <!-- Gridi 7 x 7-->
    <Grid >
        <Grid.Resources>
            <oma:HalfValueConverter x:Key="HalfValue" />
        </Grid.Resources>

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <!-- <ColumnDefinition Width="22*"/> -->
        </Grid.ColumnDefinitions>




        <!-- Käytetään omaa OwnCanvas-luokkaa, joka mahdollistaa piirtämisen solujen keskelle HalfWidth- ja HalfHeight-propertyjen avulla -->
        <oma:OwnCanvas Grid.Column="5" x:Name="canvas" Grid.Row="0">
            <Line Style="{StaticResource FullHorizontalLine}"/>
        </oma:OwnCanvas>

        <oma:OwnCanvas Grid.Column="4" Grid.Row="0">
            <Line Style="{StaticResource FullHorizontalLine}"/>
        </oma:OwnCanvas>

        <oma:OwnCanvas Grid.Column="3" Grid.Row="0">
            <Line Style="{StaticResource FullHorizontalLine}"/>
            <Line X1= "{Binding HalfWidth, ElementName=canvas}" Y1="{Binding HalfHeight, ElementName=canvas}" X2="{Binding HalfWidth, ElementName=canvas}" Y2="{Binding ActualHeight, ElementName=canvas}" Stroke="Black" StrokeThickness="4"/>

            <NappulaEllipsi:UserControl1 MouseDown="e22_MouseDown" x:Name="e2" >
                <NappulaEllipsi:UserControl1.Background>
                    <MultiBinding Converter="{StaticResource ShowNaapuritConverter}"
                             ConverterParameter="1">
                        <Binding ElementName="e25" Path="OnkoPoistettu" />
                        <Binding ElementName="e2" Path="OnkoKentalla" />
                        <Binding ElementName="e1" Path="Valittu" />
                        <Binding ElementName="e3" Path="Valittu" />
                        <Binding ElementName="e5" Path="Valittu" />
                    </MultiBinding>
                </NappulaEllipsi:UserControl1.Background>

                <NappulaEllipsi:UserControl1.InputBindings>
                    <MouseBinding Gesture="LeftClick" Command="{Binding e22_MouseLeftButtonDown}"/>
                </NappulaEllipsi:UserControl1.InputBindings>
                <Canvas.Left>
                    <MultiBinding Converter="{StaticResource HalfValue}">
                        <Binding ElementName="canvas" Path="ActualWidth" />
                        <Binding ElementName="ellipsienAiti" Path="ActualWidth" />
                    </MultiBinding>
                </Canvas.Left>
                <Canvas.Top>
                    <MultiBinding Converter="{StaticResource HalfValue}">
                        <Binding ElementName="canvas" Path="ActualHeight" />
                        <Binding ElementName="ellipsienAiti" Path="ActualHeight" />
                    </MultiBinding>
                </Canvas.Top>
            </NappulaEllipsi:UserControl1>
        </oma:OwnCanvas>

